{
  "name": "@indredk/praxis",
  "version": "0.1.1",
  "private": false,
  "description": "A collection of robust, highly-tested, and educational utility functions.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "pnpm build --watch",
    "lint": "eslint . --ext .ts",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {},
  "devDependencies": {
    "typescript": "^5.9.2"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}