{
  "name": "@indredk/praxis",
  "version": "0.1.2",
  "private": false,
  "description": "A collection of robust, highly-tested, and educational utility functions.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build:types": "tsc --project tsconfig.build.json",
    "build:js": "tsup index.ts --format cjs,esm --sourcemap --no-dts",
    "build": "pnpm clean && pnpm build:types && pnpm build:js",
    "lint": "eslint . --ext .ts",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "eslint": "^9.35.0",
    "typescript": "^5.9.2"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}