{
  "name": "praxis-monorepo",
  "version": "0.1.1",
  "private": true,
  "description": "An AI-augmented TypeScript/JavaScript utility library, focusing on robust, highly-tested, and educational implementations.",
  "author": "indredk <zys233333@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/indredk/praxis.git"
  },
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "dev:server": "turbo dev --filter=@praxis/mock-server",
    "test": "turbo test",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:ui:server": "vitest --ui packages/mock_server/",
    "test:coverage": "vitest run --coverage",
    "lint": "turbo lint",
    "clean": "turbo clean",
    "docs": "typedoc",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo build --filter=@indredk/praxis && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.7",
    "@types/node": "^20.19.13",
    "@vitest/ui": "^3.2.4",
    "chalk": "^5.6.2",
    "cross-env": "^10.0.0",
    "jsdom": "^27.0.0",
    "tsup": "^8.5.0",
    "turbo": "^2.5.6",
    "typedoc": "^0.28.12",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.15.1",
  "license": "ISC"
}